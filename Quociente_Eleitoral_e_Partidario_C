#include<stdlib.h>
#include<stdio.h>
float qe(float n1, float n2){
    return n1/n2;
}
float qp(float n1, float n2){
    return n1/n2;
}
int main(){
    float totalvotos, vagas, votospartido;
    printf("Quociente Eleitoral e Quociente Partidario\n");
    printf("\nInforme o total de votos validos: ");
    scanf("%f", &totalvotos);
    printf("\nInforme o numero de vagas a serem preenchidas: ");
    scanf("%f", &vagas);
    printf("\n");
    printf("\nO resultado do Quociente Eleitoral eh: %.0f\n", qe(totalvotos, vagas));
    printf("\n");
    printf("\nInforme o total de votos do partido: ");
    scanf("%f", &votospartido);
    printf("\n");
    printf("\nO resultado do Quociente Partidario eh: %.0f\n", qp(votospartido, qe(totalvotos, vagas)));
    return 0;
}
